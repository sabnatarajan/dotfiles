IgnoreUnknown AddKeysToAgent,UseKeychain
Match host * exec "gpg-connect-agent updatestartuptty /bye"
Include ~/.local/config/ssh/config

Host sn
  HostName github.com
  User git
  IdentitiesOnly yes
  IdentityFile ~/.ssh/yubikey_rsa.pub
  AddKeysToAgent yes
  IdentitiesOnly yes

Host gl
  HostName gitlab.com
  User git
  AddKeysToAgent yes
  IdentitiesOnly yes
  IdentityFile ~/.ssh/yubikey_rsa.pub

Host *
  IdentityFile ~/.ssh/yubikey_rsa.pub
  User sab
